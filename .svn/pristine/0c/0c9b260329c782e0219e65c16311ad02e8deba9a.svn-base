/*加载*/
.loadding .loadding-btn span, .numSpinner, .numSpinnerBox { display: inline-block; *display: inline; *zoom: 1; vertical-align: middle; }
/*加减*/
.numSpinnerBox { vertical-align: middle; display: inline-block; margin-right: 5px; }
.numSpinnerBox a { display: inline-block; vertical-align: middle; width: 25px; height: 28px; line-height: 28px; text-align: center; cursor: pointer; border: 1px solid #d2d2d2;}
.numSpinnerBox a.add:hover,.numSpinnerBox a.sub:hover {background: #ff6666; }
.numSpinnerBox a:hover i{color: #fff;}
.numSpinnerBox a i { font-size: 20px;color: #d2d2d2;}
.numSpinnerBox a.sub {color: #666; }
.numSpinnerBox a.sub i { content: "-"; }
.numSpinnerBox a.add { color: #666; }
.numSpinnerBox a.add i { content: "+"; font-style: normal; }
.numSpinnerBox input { font-size: 14px; width: 44px;margin: 0 3px; height: 28px; line-height: 28px; text-align: center; border: 0; vertical-align: middle; color: #333; border: 1px solid #d2d2d2;}

.inline{display: inline-block;*display: inline;*zoom: 1;}


/*当前位置*/
.cur-location { width: 1200px; margin: 0 auto; color: #666; font: normal 12px/50px "Microsoft YaHei"; }
.cur-location a { color: #666; }
.cur-location a.locat-home { background: url(../images/common/ico-locat-home.png) no-repeat 10px center; padding-left: 30px; }
.cur-location a:hover, .cur-location span { color: #2fbfe4; }

/*右侧广告位*/
.list-r-img { width: 100%; height: 150px; background: #3ea3ff;}
.list-r-img a { display: block; height: 100%; }
.list-r-img img { width: 100%; height: 100%; }
.list-r-01 { border: 1px solid #dcdcdc; background-color: #fff; border-top: 2px solid #2fbfe4; margin-bottom: 10px; }
