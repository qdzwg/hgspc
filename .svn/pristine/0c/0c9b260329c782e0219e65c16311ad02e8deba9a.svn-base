/*弹窗*/
.mask,.mask1{width: 100%;height: 100%;background: rgba(0,0,0,0.5);position: fixed;left: 0;top: 0;z-index: 998;}
.tips-box{animation: layerFadeIn 0.3s;text-align: center;width: 400px;background: #fff;background: #fff;border-radius: 4px;position: fixed;top: 50%;left: 50%;margin-left: -210px;z-index: 999;padding: 0 10px 10px;box-shadow: 0px 0px 10px rgba(0,0,0,0.3);}
.tips-box .tip-header{line-height: 38px;font-size: 18px;border-bottom: 1px solid #ccc;position:relative;}
.tips-box .tips-content{padding: 10px;font-size: 16px;}
.tips-box .layer-btn{  padding:16px 0 10px;  text-align: center;  }
.tips-box i.tip{margin-bottom: 15px;width: 50px;height: 50px;display: inline-block;*display:inline;*zoom: 1;background-size: cover;background-repeat: no-repeat;}
.tips-box i.tip.suc{background-image: url("../images/common/tip-suc.png");}
.tips-box i.tip.err{background-image: url("../images/common/tip-err.png");}
.tips-box .btn{margin: 0 10px;display: inline-block;width:100px;height:36px;line-height:36px;text-align: center;color:#fff;font-size: 14px;border-radius: 5px;cursor: pointer;border:none;outline: none;}
.layer-close{position: absolute;right: 10px;top: 4px;font-size: 20px;color: #d02f30;height: 30px;line-height: 30px;cursor: pointer;}
@keyframes layerFadeIn{
	0% {
		opacity: 0;
		transform: scale(.5)
	}
	100% {
		opacity: 1;
		transform: scale(1)
	}
}
.xy-box{width: 600px;margin-left: -300px;margin-top: -285px;display: none;animation: layerFadeIn 0.3s;text-align: center;background: #fff;border-radius: 4px;position: fixed;top: 50%;left: 50%;z-index: 999;padding: 0 10px 10px;box-shadow: 0px 0px 10px rgba(0,0,0,0.3);}
.xy-box .tip-header{line-height: 38px;font-size: 18px;border-bottom: 1px solid #ccc;position:relative;}
.xy-box .tips-content{height: 500px;overflow-y: scroll;padding: 10px;font-size: 16px;}


